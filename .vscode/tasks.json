{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run current file with FreeCADCmd",
            "type": "shell",
            "command": "C:/Program Files/FreeCAD 1.1/bin/FreeCADCmd.exe",
            "args": [
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "group": "build",
            "problemMatcher": []
        }
    ]
}
